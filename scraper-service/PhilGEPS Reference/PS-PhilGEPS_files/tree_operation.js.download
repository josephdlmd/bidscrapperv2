function assign_approver(userData='')
{	
	if(userData != '')
	{
		$.ajax({
			type: "POST",
			url: site_url+"/buyerUsers/getTreeUserInfo/"+userData, 
			data: {},
			cache: false,
			async:true, 
			headers : {
				'X-CSRF-Token': $('[name="_csrfToken"]').val()
			},
			success: function(data)
			{ 	 
				data = data.trim();
				alert(data);
				$('#approver_id').val(userData);
				document.getElementById('user_info').innerHTML = data; 
				$('#facebox_div').modal('hide');	
			}
		});
	}
}	